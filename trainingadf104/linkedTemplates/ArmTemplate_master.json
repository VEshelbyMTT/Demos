{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "trainingadf104"
		},
		"ls_Synapse_taxi_Sql_pool_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_Synapse_taxi_Sql_pool'"
		},
		"ls_transport_transformation_adl2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_transport_transformation_adl2'"
		},
		"ls_NYC_Cab_Repository_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://s3.amazonaws.com/nyc-tlc/"
		},
		"ls_NYC_Cab_Repository_2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nyc-tlc.s3.amazonaws.com/"
		},
		"ls_transport_transformation_adl2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://traininglake104.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "trainingadf104_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_Synapse_taxi_Sql_pool_connectionString": {
						"value": "[parameters('ls_Synapse_taxi_Sql_pool_connectionString')]"
					},
					"ls_transport_transformation_adl2_accountKey": {
						"value": "[parameters('ls_transport_transformation_adl2_accountKey')]"
					},
					"ls_NYC_Cab_Repository_properties_typeProperties_url": {
						"value": "[parameters('ls_NYC_Cab_Repository_properties_typeProperties_url')]"
					},
					"ls_NYC_Cab_Repository_2_properties_typeProperties_url": {
						"value": "[parameters('ls_NYC_Cab_Repository_2_properties_typeProperties_url')]"
					},
					"ls_transport_transformation_adl2_properties_typeProperties_url": {
						"value": "[parameters('ls_transport_transformation_adl2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}